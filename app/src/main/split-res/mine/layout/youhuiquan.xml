<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="@color/color_white"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include
        android:id="@+id/youhuiquan_include"
        layout="@layout/indent_tijiao_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <ListView
                android:divider="@null"
                android:scrollbars="none"
                android:listSelector="@color/color_white"
                android:id="@+id/list_view_youhuiquan"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>
            <LinearLayout
                android:id="@+id/ll_not_indent"
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="@dimen/yiyisan"
                android:layout_centerHorizontal="true"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/iv_collection_logo"
                    android:layout_gravity="center_horizontal"
                    android:src="@mipmap/zanwu_youhuiquan"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/tv_collection"
                    android:layout_marginTop="@dimen/shiwu"
                    android:textSize="@dimen/text_fifteensp"
                    android:layout_gravity="center_horizontal"
                    android:textColor="@color/_9999"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
        </RelativeLayout>

    </FrameLayout>

</LinearLayout>