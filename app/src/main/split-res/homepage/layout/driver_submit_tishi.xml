<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical">

    <WebView
        android:id="@+id/wb_driver_submit_tishi_url"
        android:paddingTop="@dimen/shiyi"
        android:layout_width="@dimen/sanbai"
        android:layout_height="@dimen/sanbaier"
        android:layout_gravity="center">
    </WebView>
    <LinearLayout
        android:id="@+id/ll_tishi_close"
        android:layout_width="@dimen/sanbai"
        android:background="@color/color_white"
        android:gravity="center"
        android:layout_height="wrap_content">
        <TextView
            android:gravity="center"
            android:layout_width="wrap_content"
            android:text="@string/zhidoale"
            android:textSize="@dimen/text_fourteensp"
            android:padding="@dimen/shiwu"
            android:textColor="@color/colorOrange"
            android:layout_height="wrap_content" />
    </LinearLayout>


</LinearLayout>