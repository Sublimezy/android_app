<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="#f9463c"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ImageView
        android:id="@+id/ll_test_license_back"
        android:layout_marginTop="@dimen/sanshier"
        android:layout_gravity="center_vertical"
        android:src="@mipmap/test_back"
        android:layout_width="50dp"

        android:layout_height="wrap_content" />


    <TextView
        android:id="@+id/tv_achievement_title"
        android:layout_marginTop="@dimen/ershijiu"
        android:layout_centerHorizontal="true"
        android:textColor="@color/color_white"
        android:textSize="@dimen/text_eightteensp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <ImageView
        android:id="@+id/ll_test_license_about"
        android:layout_marginTop="@dimen/sanshier"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/all_zouyou"
        android:layout_marginLeft="@dimen/slide_top_and_left_two"
        android:layout_gravity="center_vertical"
        android:src="@mipmap/win_money_about"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</RelativeLayout >