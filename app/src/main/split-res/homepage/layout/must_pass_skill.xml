<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include
        android:id="@+id/must_pass_skill_include"
        layout="@layout/new_title_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
        <RelativeLayout
            android:gravity="center"
            android:layout_below="@+id/must_pass_skill_include"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <WebView
                android:id="@+id/pass_web_view"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>
            <TextView
                android:id="@+id/tv_baoming"
                android:layout_alignParentBottom="true"
                android:layout_width="280dp"
                android:layout_height="@dimen/all_include_height"
                android:layout_centerHorizontal="true"
                android:gravity="center"
                android:layout_marginBottom="@dimen/sanshi"
                android:background="@drawable/button_shape"
                android:text="立即报名"
                android:visibility="gone"
                android:textColor="@color/color_white"
                android:textSize="25sp" />
        </RelativeLayout>
</RelativeLayout>